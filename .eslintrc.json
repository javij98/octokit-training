{
    "env": {
        "browser": false,
        "es2021": true
    },
    "extends": "standard",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "camelcase": "off",
        "quotes": "off",
        "semi":"off",
        "space-before-function-paren": "off"
    }
}
